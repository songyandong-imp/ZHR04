<mvc:View                                                                                                                                                                                                                                                      
        controllerName="com.sap.demo.sapui5admin.controller.App"                                                                                                                                                                                               
        xmlns:mvc="sap.ui.core.mvc"                                                                                                                                                                                                                            
        xmlns:tnt="sap.tnt"                                                                                                                                                                                                                                    
        xmlns="sap.m">                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
    <tnt:ToolPage                                                                                                                                                                                                                                              
            id="app"                                                                                                                                                                                                                                           
            class="sapUiDemoToolPage">                                                                                                                                                                                                                         
        <tnt:header>                                                                                                                                                                                                                                           
            <tnt:ToolHeader>                                                                                                                                                                                                                                   
                <Button                                                                                                                                                                                                                                        
                        id="sideNavigationToggleButton"                                                                                                                                                                                                        
                        icon="sap-icon://menu2"                                                                                                                                                                                                                
                        type="Transparent"                                                                                                                                                                                                                     
                        press=".onSideNavButtonPress"                                                                                                                                                                                                          
                        tooltip="{i18n>collpaseMenuButtonText}"                                                                                                                                                                                                
                        visible="false">                                                                                                                                                                                                                       
                    <layoutData>                                                                                                                                                                                                                               
                        <OverflowToolbarLayoutData priority="NeverOverflow" />                                                                                                                                                                                 
                    </layoutData>                                                                                                                                                                                                                              
                </Button>                                                                                                                                                                                                                                      
                <ToolbarSpacer />                                                                                                                                                                                                                              
                <Title                                                                                                                                                                                                                                         
                        text="{i18n>appTitle}"                                                                                                                                                                                                                 
                        level="H2" />                                                                                                                                                                                                                          
                <ToolbarSpacer />                                                                                                                                                                                                                              
                <Button                                                                                                                                                                                                                                        
                        id="errorButton"                                                                                                                                                                                                                       
                        icon="sap-icon://message-popup"                                                                                                                                                                                                        
                        visible="{= ${alerts>/alerts/errors}.length === 0 ? false : true }"                                                                                                                                                                    
                        type="Transparent"                                                                                                                                                                                                                     
                        press=".onMessagePopoverPress"                                                                                                                                                                                                         
                        tooltip="{i18n>errorButtonTooltip}">                                                                                                                                                                                                   
                    <layoutData>                                                                                                                                                                                                                               
                        <OverflowToolbarLayoutData closeOverflowOnInteraction="false" />                                                                                                                                                                       
                    </layoutData>                                                                                                                                                                                                                              
                </Button>                                                                                                                                                                                                                                      
                <Button                                                                                                                                                                                                                                        
                        id="notificationButton"                                                                                                                                                                                                                
                        icon="sap-icon://ui-notifications"                                                                                                                                                                                                     
                        visible="{= ${alerts>/alerts/notifications}.length === 0 ? false : true }"                                                                                                                                                             
                        type="Transparent"                                                                                                                                                                                                                     
                        press=".onNotificationPress"                                                                                                                                                                                                           
                        tooltip="{i18n>notificationButtonTooltip}">                                                                                                                                                                                            
                    <layoutData>                                                                                                                                                                                                                               
                        <OverflowToolbarLayoutData closeOverflowOnInteraction="false" />                                                                                                                                                                       
                    </layoutData>                                                                                                                                                                                                                              
                </Button>                                                                                                                                                                                                                                      
                <Button                                                                                                                                                                                                                                        
                        id="userButton"                                                                                                                                                                                                                        
                        text="{i18n>userName}"                                                                                                                                                                                                                 
                        type="Transparent"                                                                                                                                                                                                                     
                        press=".onUserNamePress">                                                                                                                                                                                                              
                    <layoutData>                                                                                                                                                                                                                               
                        <OverflowToolbarLayoutData closeOverflowOnInteraction="false" />                                                                                                                                                                       
                    </layoutData>                                                                                                                                                                                                                              
                </Button>                                                                                                                                                                                                                                      
            </tnt:ToolHeader>                                                                                                                                                                                                                                  
        </tnt:header>                                                                                                                                                                                                                                          
        <tnt:sideContent>                                                                                                                                                                                                                                      
            <tnt:SideNavigation                                                                                                                                                                                                                                
                    expanded="true"                                                                                                                                                                                                                            
                    itemSelect=".onItemSelect">                                                                                                                                                                                                                
                <tnt:NavigationList id="navigationList">                                                                                                                                                                                                       
                    <tnt:NavigationListItem                                                                                                                                                                                                                    
                            text="Suppliers"                                                                                                                                                                                                                   
                            icon="supplier"                                                                                                                                                                                                                    
                            key="zsuppliers" />                                                                                                                                                                                                                
                    <tnt:NavigationListItem                                                                                                                                                                                                                    
                            text="Customers"                                                                                                                                                                                                                   
                            icon="customer"                                                                                                                                                                                                                    
                            key="zcustomers" />                                                                                                                                                                                                                
                </tnt:NavigationList>                                                                                                                                                                                                                          
            </tnt:SideNavigation>                                                                                                                                                                                                                              
        </tnt:sideContent>                                                                                                                                                                                                                                     
        <tnt:mainContents>                                                                                                                                                                                                                                     
            <Page                                                                                                                                                                                                                                              
                    id="page"                                                                                                                                                                                                                                  
                    title="page"                                                                                                                                                                                                                               
                    showHeader="false">                                                                                                                                                                                                                        
                <VBox>                                                                                                                                                                                                                                         
                    <TabContainer                                                                                                                                                                                                                              
                            id="myTabContainer"                                                                                                                                                                                                                
                            class="sapUiResponsiveContentPadding"                                                                                                                                                                                              
                            itemClose="onItemClosed">                                                                                                                                                                                                          
                        <items>                                                                                                                                                                                                                                
                            <TabContainerItem name="日期">                                                                                                                                                                                                       
                                <content>                                                                                                                                                                                                                      
                                    <DatePicker                                                                                                                                                                                                                
                                            id="date"                                                                                                                                                                                                          
                                            width="200px"                                                                                                                                                                                                      
                                            displayFormat="yyyy-MM-dd"                                                                                                                                                                                         
                                            valueFormat="yyyyMMddHHmmss" />                                                                                                                                                                                    
                                </content>                                                                                                                                                                                                                     
                            </TabContainerItem>                                                                                                                                                                                                                
                        </items>                                                                                                                                                                                                                               
                    </TabContainer>                                                                                                                                                                                                                            
                </VBox>                                                                                                                                                                                                                                        
            </Page>                                                                                                                                                                                                                                            
        </tnt:mainContents>                                                                                                                                                                                                                                    
    </tnt:ToolPage>                                                                                                                                                                                                                                            
</mvc:View>                                                                                                                                                                                                                                                    